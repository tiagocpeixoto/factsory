{
  "name": "factsory",
  "description": "A kind of factory and DI patterns library targeted to Typescript (w/o decorators)",
  "version": "3.1.0",
  "main": "./dist/cjs/src/index.js",
  "types": "./dist/cjs/src/index.d.ts",
  "module": "./dist/mjs/src/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/src/index.js",
      "import": "./dist/mjs/src/index.js",
      "default": "./dist/cjs/src/index.js"
    },
    "./mjs/*": "./dist/mjs/src/*",
    "./cjs/*": "./dist/cjs/src/*",
    "./*": "./dist/cjs/src/*"
  },
  "files": [
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "author": {
    "name": "Tiago da Costa Peixoto",
    "email": "tiagocpeixoto@gmail.com"
  },
  "license": "MIT",
  "repository": "github:tiagocpeixoto/factsory",
  "bugs": {
    "url": "https://github.com/tiagocpeixoto/factsory/issues"
  },
  "homepage": "https://github.com/tiagocpeixoto/factsory",
  "keywords": [
    "typescript",
    "factory",
    "factory-pattern",
    "gof",
    "gof-pattern",
    "gof-factory",
    "gof-factory-pattern",
    "pattern",
    "pattern-library",
    "pattern-lib"
  ],
  "scripts": {
    "lint": "eslint \"src/**\"",
    "prettier:write": "yarn prettier --write src/**/*.ts",
    "prettier:check": "yarn prettier --check src/**/*.ts",
    "test": "yarn build && jest --config=jest.config.ts --detectOpenHandles --silent",
    "test:clear": "jest --clearCache",
    "validate": "yarn test && yarn lint && yarn prettier:check",
    "build": "tsc --build && yarn build:esm",
    "build:esm": "tsc --build tsconfig.esm.json",
    "clean": "yarn build --clean",
    "_postpublish": "git tag -as ${npm_package_version} -m \"Release version ${npm_package_version}\" && git push && git push --tags"
  },
  "dependencies": {
    "async-mutex": "~0.4.0",
    "tslib": "~2.4.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/chai": "~4.3.4",
    "@types/chai-like": "~1.1.1",
    "@types/chai-things": "~0.0.35",
    "@types/jest": "~29.2.4",
    "@types/node": "16.18.11",
    "@typescript-eslint/eslint-plugin": "~5.47.1",
    "@typescript-eslint/parser": "~5.47.1",
    "allure-commandline": "~2.20.1",
    "chai": "~4.3.7",
    "chai-like": "~1.1.1",
    "chai-things": "~0.2.0",
    "dotenv-safe": "~8.2.0",
    "eslint": "~8.30.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-config-standard": "~17.0.0",
    "eslint-plugin-etc": "~2.0.2",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "~27.1.7",
    "eslint-plugin-jest-formatting": "~3.1.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-standard": "~5.0.0",
    "jest": "~29.3.1",
    "jest-allure": "~0.1.3",
    "jest-docblock": "~29.2.0",
    "jest-mock-extended": "~3.0.1",
    "jest-runner": "~29.3.1",
    "jest-runner-groups": "~2.2.0",
    "node-notifier": "~10.0.1",
    "prettier": "~2.8.1",
    "ts-jest": "~29.0.3",
    "ts-node": "~10.9.1",
    "typescript": "4.9.4"
  },
  "packageManager": "yarn@3.3.1"
}
